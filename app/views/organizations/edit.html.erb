<h2> Edit Details for <%= @organization.name %> </h2>
<%=form_with(model: @organization) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :hourly_rate %>
  <%= f.number_field :hourly_rate, step: :any %>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= button_to "Update", organizations_path(name: :name, hourly_rate: :hourly_rate), method: :patch %>
<% end %>
